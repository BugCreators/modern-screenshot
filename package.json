{
  "name": "egami",
  "version": "1.1.1",
  "packageManager": "pnpm@7.0.0",
  "description": "Generate image using HTML5 canvas and SVG",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/egami.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/egami/issues"
  },
  "homepage": "https://github.com/qq15725/egami",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "egami",
    "image",
    "dom",
    "svg",
    "canvas",
    "screenshot"
  ],
  "license": "MIT",
  "author": "wxm",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp package.json --commit \"chore(release): publish v%s\" --push --tag && pnpm publish"
  },
  "devDependencies": {
    "@qq15725/eslint-config": "^1.1.0",
    "bumpp": "^7.2.0",
    "eslint": "^8.17.0",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4",
    "vitest": "^0.12.10"
  }
}
